<?xml version="1.0" encoding="UTF-8"?>
<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd">
	<modelVersion>4.0.0</modelVersion>

	<groupId>com.synchronoss</groupId>
	<artifactId>saw-services</artifactId>
	<version>1.0</version>
	<packaging>pom</packaging>
	
	<name>Data Processing Framework</name>
	<url>http://www.synchronoss.com</url>
	
	<inceptionYear>2016</inceptionYear>
	<scm>
		<connection>scm:svn:http://none</connection>
		<url>scm:svn:https://none</url>
	</scm>
	
	<properties>
		<!-- output.directory>${output}</output.directory -->
  	    	<project.build.sourceEncoding>UTF-8</project.build.sourceEncoding>
		<!-- Versions of 3rd party dependencies  -->
		<spark-version>1.6.1</spark-version>
		<elasticsearch.version>1.7.3</elasticsearch.version>
		<elasticsearch-spark.version>2.4.0</elasticsearch-spark.version>
		<parquet.version>1.6.0</parquet.version>
		<junit.version>4.6</junit.version>
		<date4j.version>1.5.1</date4j.version>
		<jxel.version>2.1.1</jxel.version>
		<opencsv.version>2.3</opencsv.version>
		<build.number.plugin>1.1</build.number.plugin>
		<enforcer.version>1.3.1</enforcer.version>
		<maven.assembly.version>2.4</maven.assembly.version>
		<maven.jar.version>2.4</maven.jar.version>
		<maven.compile.version>2.3.2</maven.compile.version>
		<maven.compiler.source>1.8</maven.compiler.source>
		<maven.compiler.target>1.8</maven.compiler.target>
		<jackson.databind>2.4.1.3</jackson.databind>
		<jackson.core>2.4.1.1</jackson.core>
		<jackson.annotation>2.4.0</jackson.annotation>
		<http.version>4.3.4</http.version>
		<log4j.version>1.2.17</log4j.version>
		<slfj.version>1.7.6</slfj.version>
		<ezmorph.version>0.8.1</ezmorph.version>
		<amazon.s3>1.0</amazon.s3>
		<kyro>3.0.0</kyro>
		<scala.version>2.10</scala.version>
		<common.lang>2.6</common.lang>
		<json.vesion>20080701</json.vesion>		
		<guava.version>20.0</guava.version>
		<jdk.version>1.8</jdk.version>
		<snappy.version>1.0.4.1</snappy.version>
		<commons-collections.version>3.2.1</commons-collections.version>
		<mapr.core.version>5.2.0</mapr.core.version>		
		<mapr.hadoop.client.version>2.7.0-mapr-1607</mapr.hadoop.client.version>		
		<rpm.saw.root>/opt</rpm.saw.root>
	</properties>
	<repositories>
		<repository>
			<id>maven2-repo</id>
			<url>http://repo1.maven.apache.org/maven2</url>
		</repository>
		<repository>
			<id>mapr-maven</id>
			<url>http://repository.mapr.com/maven</url>
			<releases>
				<enabled>true</enabled>
			</releases>
			<snapshots>
				<enabled>false</enabled>
			</snapshots>
		</repository>
		<repository>
			<id>mapr-maven2</id>
			<url>http://repository.mapr.com/nexus/content/groups/mapr-public/releases</url>
			<releases>
				<enabled>true</enabled>
			</releases>
			<snapshots>
				<enabled>false</enabled>
			</snapshots>
		</repository>
		<repository>
			<id>typesafe-releases</id>
			<url>http://repo.typesafe.com/typesafe/releases/</url>
		</repository>
		<repository>
			<id>typesafe-snapshots</id>
			<url>http://repo.typesafe.com/typesafe/snapshots/</url>
		</repository>
		<repository>
			<id>mapr-releases</id>
			<url>http://repository.mapr.com/maven/</url>
			<snapshots>
				<enabled>true</enabled>
			</snapshots>
			<releases>
				<enabled>true</enabled>
			</releases>
		</repository>
	</repositories>
	<pluginRepositories>
		<pluginRepository>
			<id>typesafe-releases-plugins</id>
			<url>http://repo.typesafe.com/typesafe/releases/</url>
			<snapshots>
				<enabled>false</enabled>
			</snapshots>
		</pluginRepository>
	</pluginRepositories>
	<modules>
		<module>saw-common</module>	
		<module>saw-metadata-service</module>	
		<module>saw-business-service</module>			
		<module>saw-transport-service</module>					
	</modules> 
</project>