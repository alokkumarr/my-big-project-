component [MariaDB Client] as MariaDbClient

component [MariaDB (instance #1)] as MariaDb1
component [MariaDB (instance #2)] as MariaDb2
component [MariaDB (instance #N)] as MariaDbN
MariaDb1 <--> MariaDb2 : replicate
MariaDb2 <--> MariaDbN : replicate

MariaDbClient .> MariaDb1
MariaDbClient .> MariaDb2
MariaDbClient .> MariaDbN
