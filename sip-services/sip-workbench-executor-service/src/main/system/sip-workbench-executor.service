[Unit]
Description=SIP Workbench Executor

[Service]
Type=notify
NotifyAccess=all
ExecStart=/opt/bda/sip-executor/bin/run-workbench-executor
SyslogIdentifier=sip-workbench-executor
User=mapr
Restart=always
Environment="JAVA_OPTS=-Dsaw.workbench.executor=%i"
EnvironmentFile=/etc/bda/mapr-ticket.env
SuccessExitStatus=143
TimeoutSec=600
KillMode=mixed
[Install]
WantedBy=sip-workbench.service

