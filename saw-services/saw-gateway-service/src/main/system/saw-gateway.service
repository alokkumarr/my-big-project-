[Unit]
Description=SAW Gateway

[Service]
ExecStart=/bin/sh -c "exec java -Xmx1G -jar /opt/bda/saw-gateway-service/saw-gateway.jar --spring.config.location=/opt/bda/saw-gateway-service/conf/application.yml"
SyslogIdentifier=saw-gateway
User=mapr
Restart=on-failure

[Install]
WantedBy=default.target
