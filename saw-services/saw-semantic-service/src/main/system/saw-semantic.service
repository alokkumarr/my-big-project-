[Unit]
Description=SAW Semantic

[Service]
Type=notify
ExecStart=/bin/sh -c "exec java -Xmx1G -Dloader.path=BOOT-INF/lib,$(sip-mapr-classpath) -jar /opt/bda/saw-semantic-service/saw-semantic.jar --spring.config.location=/opt/bda/saw-semantic-service/conf/application.yml"
TimeoutStartSec=300
SyslogIdentifier=saw-semantic
User=mapr
Restart=on-failure
# Allow service to exit on SIGTERM, if socket-activated
SuccessExitStatus=143
