            <!-- Assembly plugin : end -->

            <plugin>
              <groupId>de.dentrassi.maven</groupId>
              <artifactId>rpm</artifactId>
              <version>0.10.0</version>
              <executions>
                <execution>
                  <goals>
                    <goal>rpm</goal>
                  </goals>
                  <configuration>
                    <packageName>saw-transport</packageName>
                    <version>${revision.nodash}</version>
                    <skipSigning>true</skipSigning>
                    <rulesets>
                      <ruleset>
                        <id>default</id>
                        <rules>
                          <rule>
                            <user>${app.user}</user>
                            <group>${app.group}</group>
                            <mode>0444</mode>
                          </rule>
                          <rule>
                            <when>
                              <type>directory</type>
                            </when>
                            <mode>0750</mode>
                          </rule>
                          <rule>
                            <when>
                              <prefix>${rpm.ts.root}/bin/</prefix>
                            </when>
                            <mode>0755</mode>
                          </rule>
                          <rule>
                            <when>
                              <prefix>${rpm.ts.root}/conf/</prefix>
                            </when>
                            <mode>0644</mode>
                          </rule>
                          <rule>
                            <when>
                              <prefix>/usr/lib/systemd/system/</prefix>
                            </when>
                            <mode>0644</mode>
                          </rule>
                          <rule>
                            <when>
                              <prefix>/var/saw/service/data/</prefix>
                            </when>
                            <mode>0644</mode>
                          </rule>
                        </rules>
                      </ruleset>
                    </rulesets>
                    <entries>
                      <entry>
                        <name>${rpm.ts.root}/bin</name>
                        <directory>true</directory>
                        <ruleset>default</ruleset>
                      </entry>
                      <entry>
                        <name>${rpm.ts.root}/lib</name>
                        <directory>true</directory>
                        <ruleset>default</ruleset>
                      </entry>
                      <entry>
                        <name>${rpm.ts.root}/conf</name>
                        <directory>true</directory>
                        <ruleset>default</ruleset>
                      </entry>
                      <entry>
                        <name>${rpm.ts.root}/sparklib</name>
                        <directory>true</directory>
                        <ruleset>default</ruleset>
                      </entry>
                      <entry>
                        <name>${rpm.ts.root}/schema</name>
                        <directory>true</directory>
                        <ruleset>default</ruleset>
                      </entry>
                      <entry>
                        <name>${rpm.ts.root}/bin</name>
                        <collect>
                          <from>scripts</from>
                        </collect>
                        <ruleset>default</ruleset>
                      </entry>
                      <entry>
                        <name>${rpm.ts.root}/bin</name>
                        <collect>
                          <from>${project.basedir}/../saw-metadata-service/src/main/scripts</from>
                        </collect>
                        <ruleset>default</ruleset>
                      </entry>
                      <entry>
                        <name>${rpm.ts.root}/bin</name>
                        <collect>
                          <from>${project.basedir}/../saw-data-access/src/main/scripts</from>
                        </collect>
                        <ruleset>default</ruleset>
                      </entry>
                      <entry>
                        <name>${rpm.ts.root}/conf</name>
                        <collect>
                          <from>conf</from>
                        </collect>
                        <ruleset>default</ruleset>
                      </entry>
                      <entry>
                        <name>${rpm.ts.root}/lib</name>
                        <collect>
                          <from>target/${project.artifactId}-${project.version}-package/${project.artifactId}-${project.version}/lib</from>
                        </collect>
                        <ruleset>default</ruleset>
                      </entry>
                      <entry>
                        <name>${rpm.ts.root}/sparklib</name>
                        <collect>
                          <from>target/${project.artifactId}-${project.version}-package/${project.artifactId}-${project.version}/sparklib</from>
                        </collect>
                        <ruleset>default</ruleset>
                      </entry>
                      <entry>
                        <name>${rpm.ts.root}/schema</name>
                        <collect>
                          <from>${project.basedir}/../json-schema</from>
                        </collect>
                      </entry>
                      <entry>
                        <name>/var/saw/service/data</name>
                        <directory>true</directory>
                        <mode>0755</mode>
                        <ruleset>default</ruleset>
                      </entry>
                      <entry>
                        <name>/var/saw/service/run</name>
                        <directory>true</directory>
                        <mode>0755</mode>
                        <ruleset>default</ruleset>
                      </entry>
                      <entry>
                        <name>/var/saw/service/data/C_ui_analyze_sample.json</name>
                        <file>${project.basedir}/../saw-metadata-service/src/test/resources/C_ui_analyze_sample.json</file>
                        <ruleset>default</ruleset>
                      </entry>
                      <entry>
                        <name>/var/saw/service/data/D_ui_analyze_sample.json</name>
                        <file>${project.basedir}/../saw-metadata-service/src/test/resources/D_ui_analyze_sample.json</file>
                        <ruleset>default</ruleset>
