{
  "extends": [
    "tslint-config-xo/esnext",
    "angular-tslint-rules"
  ],
  "rules": {
    "indent": [true, "spaces"],
    "quotemark": [true, "single"],
    "object-literal-sort-keys": false,
    "trailing-comma": [true, {"multiline": "never", "singleline": "never"}],
    "max-params": ["error", 20],
    "no-warning-comments": [0]
  },
  "jsRules": {
    "indent": [true, "spaces"],
    "quotemark": [true, "single"],
    "object-literal-sort-keys": false,
    "trailing-comma": [true, {"multiline": "never", "singleline": "never"}],
    "max-params": ["error", 20],
    "no-warning-comments": [0]
  }
}
